{
  "compilerOptions": {
    "module": "commonjs",
    "target": "esnext",
    "jsx": "preserve",
    "lib": ["dom", "esnext"],
    "strict": true,
    "noEmit": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "noUnusedLocals": false,
    "allowJs": true
  },
  "include": [
    "src",
    "src/types/**/*.d.ts",
    "gatsby-browser.js",
    "gatsby-config.js",
    "gatsby-ssr.js",
    "gatsby-node.js"
  ],
  "exclude": ["node_modules", "public", ".cache"]
}
